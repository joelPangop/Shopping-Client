<?xml version='1.0' encoding='utf-8'?>
<widget version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <access origin="*" />
  
  <feature name="CardIO">
    <param name="ios-package" value="CardIOCordovaPlugin" onload="false"/>
  </feature>

  <feature name="PhotoViewer">
    <param name="ios-package" value="PhotoViewer"/>
  </feature>

  <feature name="PayPalMobile">
    <param name="ios-package" value="PayPalMobileCordovaPlugin" onload="true"/>
  </feature>

  <feature name="AccountKitPlugin">
    <param name="ios-package" value="AKPlugin" onload="true"/>
  </feature>

  <feature name="CordovaHttpPlugin">
    <param name="ios-package" value="CordovaHttpPlugin"/>
  </feature>

  <feature name="Camera">
    <param name="ios-package" value="CDVCamera"/>
  </feature>

  <feature name="Notification">
    <param name="ios-package" value="CDVNotification"/>
  </feature>

  <feature name="FacebookConnectPlugin">
    <param name="ios-package" value="FacebookConnectPlugin"/>
    <param name="onload" value="true"/>
  </feature>

  <feature name="File">
    <param name="ios-package" value="CDVFile"/>
    <param name="onload" value="true"/>
  </feature>

  <feature name="FileTransfer">
    <param name="ios-package" value="CDVFileTransfer"/>
  </feature>

  <feature name="FirebaseAuthentication">
    <param name="ios-package" value="FirebaseAuthenticationPlugin"/>
    <param name="onload" value="false"/>
  </feature>

  <feature name="FirebasePlugin">
    <param name="ios-package" value="FirebasePlugin"/>
    <param name="onload" value="true"/>
  </feature>

  <feature name="Globalization">
    <param name="ios-package" value="CDVGlobalization"/>
  </feature>

  <feature name="NativeStorage">
    <param name="ios-package" value="NativeStorage"/>
  </feature>

  <feature name="NetworkStatus">
    <param name="ios-package" value="CDVConnection"/>
  </feature>

  <feature name="networkinterface">
    <param name="ios-package" value="CDVNetworkInterface" onload="true"/>
  </feature>

  <feature name="CordovaStripe">
    <param name="ios-package" value="CordovaStripe"/>
  </feature>

  <feature name="ImagePicker">
    <param name="ios-package" value="SOSPicker"/>
  </feature>

  <feature name="CDVWKWebViewFileXhr">
    <param name="ios-package" value="CDVWKWebViewFileXhr"/>
    <param name="onload" value="true"/>
  </feature>

  <feature name="SocialSharing">
    <param name="ios-package" value="SocialSharing"/>
    <param name="onload" value="true"/>
  </feature>

  <feature name="Sms">
    <param name="ios-package" value="Sms"/>
  </feature>

  <feature name="IonicDeeplinkPlugin">
    <param name="ios-package" value="IonicDeeplinkPlugin" onload="true"/>
  </feature>

  <feature name="Device">
    <param name="ios-package" value="CDVDevice"/>
  </feature>

  
</widget>